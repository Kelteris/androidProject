<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:bottom="-14dp"
        android:right="-14dp"
        android:start="1dp"
        android:top="1dp">
        <shape>
            <solid android:color="#00ffffff" />
            <stroke
                android:width="14dp"
                android:color="#acacac" />
        </shape>
    </item>

    <item>
        <shape>
            <corners android:topLeftRadius="@dimen/hour_radius" />
            <solid android:color="#00ffffff" />
            <stroke
                android:width="1dp"
                android:color="#000000" />
        </shape>
    </item>
    <item
        android:bottom="-14dp"
        android:right="-14dp"
        android:start="5dp"
        android:top="5dp">
        <shape>
            <corners android:topLeftRadius="@dimen/hour_radius" />
            <solid android:color="#00ffffff" />
            <stroke
                android:width="1dp"
                android:color="#000000" />
        </shape>
    </item>
</layer-list>