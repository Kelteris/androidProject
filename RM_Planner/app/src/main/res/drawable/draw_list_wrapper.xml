<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:left="-2dp"
        android:top="0dp"
        android:right="-16dp"
        android:bottom="-16dp">
        <shape>
            <solid android:color="#00ffffff" />
            <stroke
                android:width="15dp"
                android:color="#acacac" />
        </shape>
    </item>
    <item
        android:top="-4dp">
        <shape>
            <corners android:bottomRightRadius="@dimen/list_radius"/>
            <solid android:color="#00ffffff" />
            <stroke
                android:width="1dp"
                android:color="#000000" />
        </shape>
    </item>
</layer-list>